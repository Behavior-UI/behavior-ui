.spinner {
	position: absolute;
	opacity: 0.9;
	filter: alpha(opacity=90);
	-ms-filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=90);
	z-index: 999;
	background: #fff;
}
.spinner-msg {
	text-align: center;
	font-weight: bold;
}

.spinner-img {
	background: url(<%= asset_path 'behavior_ui/js/Tests/Interactive/_assets/spinner.gif' %>) no-repeat;
	width: 24px;
	height: 24px;
	margin: 0 auto;
}
